<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:AnySheet.SheetModule.Primitives"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AnySheet.SheetModule.Primitives.NumberBoxPrimitive"
             x:DataType="vm:NumberBoxPrimitive"
             x:Name="Control">
    <UserControl.Styles>
        <StyleInclude Source="/Assets/SheetModuleStyles.axaml" />
    </UserControl.Styles>
    <Border Name="Container" Classes="Primitive" BorderBrush="{DynamicResource Accent}" BorderThickness="2">
        <NumericUpDown Name="NumberBox" ValueChanged="ValueChanged" HorizontalAlignment="Center"
                 VerticalAlignment="Center" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" />
    </Border>
</UserControl>