<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:AnySheet.SheetModule"
             xmlns:behaviors="clr-namespace:AnySheet.Behaviors"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AnySheet.SheetModule.SheetModule"
             x:DataType="vm:SheetModule"
             x:Name="Control">
    <UserControl.Styles>
        <StyleInclude Source="../Assets/SheetModuleStyles.axaml" />
    </UserControl.Styles>
    <Interaction.Behaviors>
        <behaviors:ModuleDragBehavior GridWidth="{Binding GridSnap}" GridHeight="{Binding GridSnap}"
                                      IsEnabled="{Binding DragEnabled}"
                                      DragCompletedCommand="{ReflectionBinding DragCompletedCommand}" />
    </Interaction.Behaviors>
    <Border Name="Container" Classes="Module" BorderThickness="4">
        <Grid Name="PrimitiveGrid" RowSpacing="4" ColumnSpacing="4" VerticalAlignment="Center"
              HorizontalAlignment="Center" />
    </Border>
</UserControl>
